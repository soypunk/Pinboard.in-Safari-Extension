<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Author</key>
	<string>Jake Bell</string>
	<key>CFBundleDisplayName</key>
	<string>PinSafari</string>
	<key>CFBundleIdentifier</key>
	<string>com.iamtheunraveler.pinsafari</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>Chrome</key>
	<dict>
		<key>Global Page</key>
		<string>pin.html</string>
		<key>Toolbar Items</key>
		<array>
			<dict>
				<key>Command</key>
				<string>pinsafari</string>
				<key>Identifier</key>
				<string>pbSend</string>
				<key>Image</key>
				<string>pin.png</string>
				<key>Include By Default</key>
				<true/>
				<key>Label</key>
				<string>Save to Pinboard</string>
				<key>Palette Label</key>
				<string>Save to Pinboard</string>
				<key>Tool Tip</key>
				<string>Save to Pinboard</string>
			</dict>
		</array>
	</dict>
	<key>Content</key>
	<dict>
		<key>Scripts</key>
  		<dict>
  			<key>Start</key>
  			<array>
  				<string>popup.js</string>
  			</array>
  		</dict>
	</dict>
	<key>Description</key>
	<string>A Safari extension for posting items to pinboard.in.</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Level</key>
			<string>All</string>
		</dict>
	</dict>
	<key>Update Manifest URL</key>
	<string>http://github.com/theunraveler/Pinboard.in-Safari-Extension/raw/master/latest.plist</string>
	<key>Website</key>
	<string>http://iamtheunraveler.com</string>
</dict>
</plist>
